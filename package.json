{
  "name": "anularjs-tdd",
  "version": "1.0.0",
  "description": "AngularJs SPA with ES6 and Webpack bundler",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --mode=development",
    "build": "eslint -c .eslintrc --fix app/ && webpack",
